<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BConnectButtonView">
        <attr name="authUrl" format="string" />
        <attr name="discoveryUrl" format="string" />
        <attr name="tokenUrl" format="string" />
        <attr name="redirectUri" format="string" />
        <attr name="clientId" format="string" />
        <attr name="activeAuthentification" format="boolean" />
        <attr name="style" format="enum">
            <enum name="icon" value="0"/>
            <enum name="bPremierBlue" value="1"/>
            <enum name="bPremierWhite" value="2"/>
            <enum name="largeBlack" value="3"/>
            <enum name="largeBlue" value="4"/>
            <enum name="largeWhite" value="5"/>
            <enum name="largeWhiteBw" value="6"/>
            <enum name="largeWhiteNoBorder" value="7"/>
            <enum name="largeWhiteNoBorderBw" value="8"/>
            <enum name="logotypeBlue" value="9"/>
            <enum name="logotypeBlueBw" value="10"/>
            <enum name="logotypeWhite" value="11"/>
            <enum name="logotypeWhiteBw" value="12"/>
            <enum name="signatureBlack" value="13"/>
            <enum name="signatureBlue" value="14"/>
            <enum name="signatureOrange" value="15"/>
            <enum name="signatureWhite" value="16"/>
        </attr>
        <attr name="scope" format="string" />
    </declare-styleable>
</resources>